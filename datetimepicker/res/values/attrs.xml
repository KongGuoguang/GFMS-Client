<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NumberPicker">
        <attr name="np_textColor" format="color" />
        <attr name="np_textSize" format="float|dimension" />
        <attr name="np_startNumber" format="integer" />
        <attr name="np_endNumber" format="integer" />
        <attr name="np_currentNumber" format="integer" />
        <attr name="np_verticalSpacing" format="dimension" />
        <attr name="np_flagText" format="string|reference" />
        <attr name="np_flagTextSize" format="dimension" />
        <attr name="np_flagTextColor" format="color" />
        <attr name="np_background" format="color" />
        <attr name="np_rowNumber" format="integer" />
    </declare-styleable>

    <declare-styleable name="DateTimePicker">
        <attr name="dtp_color" format="color"/>
        <attr name="dtp_numberSize" format="dimension"/>
        <attr name="dtp_separatorSize" format="dimension"/>
        <attr name="dtp_startYear" format="integer"/>
        <attr name="dtp_endYear" format="integer"/>
        <attr name="dtp_numberSpacing" format="dimension"/>
        <attr name="dtp_dateTimeSpacing" format="dimension" />
        <attr name="dtp_background" format="color"/>
    </declare-styleable>

</resources>